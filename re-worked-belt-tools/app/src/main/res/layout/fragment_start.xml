<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".ui.fragments.StartFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/background">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp"
            android:scrollbars="none"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15dp"
                    android:background="@color/fading_background"
                    android:orientation="vertical"
                    android:padding="5dp">

                    <TextView
                        android:id="@+id/sqr_foot_In_txt"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/get_sqr_ft_in"
                        android:textSize="24sp"
                        android:textStyle="bold"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <LinearLayout
                        android:id="@+id/sqr_ft_in_layout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        app:layout_constraintTop_toBottomOf="@+id/sqr_foot_In_txt"
                        tools:layout_editor_absoluteX="5dp">

                        <EditText
                            android:id="@+id/sqrtBox1"
                            android:layout_width="65dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:ems="10"
                            android:gravity="center"
                            android:hint="@string/width"
                            android:importantForAutofill="no"
                            android:inputType="number|numberDecimal" />

                        <EditText
                            android:id="@+id/sqrtBox2"
                            android:layout_width="60dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:ems="10"
                            android:gravity="center"
                            android:hint="@string/length"
                            android:importantForAutofill="no"
                            android:inputType="number|numberDecimal" />

                    </LinearLayout>

                    <Button
                        android:id="@+id/sqr_per_room_btn"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/sqr_per_room_btn"
                        android:textColor="@color/black"
                        android:backgroundTint="@color/action_btn_color"
                        app:layout_constraintEnd_toEndOf="@+id/sqr_foot_In_txt"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/sqr_ft_in_layout" />

                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15dp"
                    android:background="@color/fading_background"
                    android:orientation="vertical"
                    android:padding="5dp">

                    <TextView
                        android:id="@+id/num_boxes_txt"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/number_of_boxes"
                        android:textSize="24sp"
                        android:textStyle="bold"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <LinearLayout
                        android:id="@+id/linearLayout2"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:orientation="horizontal"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/num_boxes_txt">

                        <EditText
                            android:id="@+id/homeSqrFt"
                            android:layout_width="10dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:ems="10"
                            android:gravity="center"
                            android:hint="@string/total_sqr_ft"
                            android:inputType="number|numberDecimal" />

                        <EditText
                            android:id="@+id/boxSqrFt"
                            android:layout_width="10dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:ems="10"
                            android:gravity="center"
                            android:hint="@string/box_sqr_ft"
                            android:inputType="number|numberDecimal" />
                    </LinearLayout>

                    <Button
                        android:id="@+id/tileBoxResultsButton"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:textColor="@color/black"
                        android:backgroundTint="@color/action_btn_color"
                        android:text="@string/tile_box_results_btn"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/linearLayout2" />

                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15dp"
                    android:background="@color/fading_background"
                    android:orientation="vertical"
                    android:padding="5dp">

                    <TextView
                        android:id="@+id/titleSqrFootToSqrIn"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:lineSpacingExtra="14sp"
                        android:text="@string/sqr_ft_to_sqr_in"
                        android:textSize="24sp"
                        android:textStyle="bold"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <EditText
                        android:id="@+id/sqrFootBox"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:ems="10"
                        android:gravity="center"
                        android:hint="@string/sqr_ft"
                        android:inputType="number|numberDecimal"
                        app:layout_constraintEnd_toStartOf="@+id/sqrFootToSqrInButton"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/titleSqrFootToSqrIn" />

                    <Button
                        android:id="@+id/sqrFootToSqrInButton"
                        android:layout_width="wrap_content"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:text="@string/both_ways_arrow"
                        android:textColor="@color/black"
                        android:backgroundTint="@color/action_btn_color"
                        app:layout_constraintBottom_toBottomOf="@+id/sqrInBox"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/titleSqrFootToSqrIn" />

                    <EditText
                        android:id="@+id/sqrInBox"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:ems="10"
                        android:gravity="center"
                        android:hint="@string/sqr_in"
                        android:inputType="number|numberDecimal"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/sqrFootToSqrInButton"
                        app:layout_constraintTop_toBottomOf="@+id/titleSqrFootToSqrIn" />

                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15dp"
                    android:background="@color/fading_background"
                    android:orientation="vertical"
                    android:padding="5dp">

                    <TextView
                        android:id="@+id/titleCutBlindWidth"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/cut_blind_width"
                        android:textSize="24sp"
                        android:textStyle="bold"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <EditText
                        android:id="@+id/windowWidthBox"
                        android:layout_width="106dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:ems="10"
                        android:gravity="center"
                        android:hint="@string/opening"
                        android:inputType="number|numberDecimal"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/titleCutBlindWidth" />

                    <EditText
                        android:id="@+id/blindWidthBox"
                        android:layout_width="99dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:ems="10"
                        android:gravity="center"
                        android:hint="@string/blind"
                        android:inputType="number|numberDecimal"
                        app:layout_constraintStart_toEndOf="@+id/windowWidthBox"
                        app:layout_constraintTop_toBottomOf="@+id/titleCutBlindWidth" />

                    <Button
                        android:id="@+id/blindWidthEqualsButton"
                        android:layout_width="60dp"
                        android:layout_height="wrap_content"
                        android:text="@string/equals_symbol"
                        android:textSize="14sp"
                        android:textColor="@color/black"
                        android:backgroundTint="@color/action_btn_color"
                        app:layout_constraintBottom_toBottomOf="@+id/blindWidthBox"
                        app:layout_constraintStart_toEndOf="@+id/blindWidthBox"
                        app:layout_constraintTop_toBottomOf="@+id/titleCutBlindWidth" />

                    <TextView
                        android:id="@+id/blindWidthResult"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:gravity="start"
                        android:layout_marginStart="16dp"
                        android:text="@string/zero"
                        android:textSize="18sp"
                        app:layout_constraintBottom_toBottomOf="@+id/blindWidthBox"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/blindWidthEqualsButton"
                        app:layout_constraintTop_toBottomOf="@+id/titleCutBlindWidth" />

                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15dp"
                    android:background="@color/fading_background"
                    android:orientation="vertical"
                    android:padding="5dp">

                    <TextView
                        android:id="@+id/titleDecimalToFraction"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/decimal_to_fraction"
                        android:textSize="24sp"
                        android:textStyle="bold"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <EditText
                        android:id="@+id/decimalBox"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:hint="@string/decimal"
                        android:inputType="number|numberDecimal"
                        app:layout_constraintEnd_toStartOf="@+id/decimalToFractionButton"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/titleDecimalToFraction" />

                    <Button
                        android:id="@+id/decimalToFractionButton"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:text="@string/both_ways_arrow"
                        android:textColor="@color/black"
                        android:backgroundTint="@color/action_btn_color"
                        app:layout_constraintBottom_toBottomOf="@+id/decimalBox"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/titleDecimalToFraction" />

                    <EditText
                        android:id="@+id/fractionBox"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:hint="@string/fraction"
                        android:inputType="date"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/decimalToFractionButton"
                        app:layout_constraintTop_toBottomOf="@+id/titleDecimalToFraction" />

                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15dp"
                    android:background="@color/fading_background"
                    android:orientation="vertical"
                    android:padding="5dp">

                    <TextView
                        android:id="@+id/titleLinealFtToSqrYard"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/lineal_ft_to_sqr_yard"
                        android:textSize="24sp"
                        android:textStyle="bold"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <EditText
                        android:id="@+id/linealFtBox"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:hint="@string/lineal_feet"
                        android:inputType="number|numberDecimal"
                        app:layout_constraintEnd_toStartOf="@+id/linealFtToSqrYardButton"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/titleLinealFtToSqrYard" />

                    <Button
                        android:id="@+id/linealFtToSqrYardButton"
                        android:layout_width="111dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/both_ways_arrow"
                        android:textColor="@color/black"
                        android:backgroundTint="@color/action_btn_color"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/titleLinealFtToSqrYard" />

                    <EditText
                        android:id="@+id/sqrYardBox"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:hint="@string/sqr_yard"
                        android:inputType="number|numberDecimal"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/linealFtToSqrYardButton"
                        app:layout_constraintTop_toBottomOf="@+id/titleLinealFtToSqrYard" />

                </androidx.constraintlayout.widget.ConstraintLayout>


                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15dp"
                    android:background="@color/fading_background"
                    android:orientation="vertical"
                    android:padding="5dp">

                    <TextView
                        android:id="@+id/textView5"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/sqr_ft_price"
                        android:textSize="24sp"
                        android:textStyle="bold"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <EditText
                        android:id="@+id/boxPriceBox"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:ems="5"
                        android:gravity="center"
                        android:hint="@string/box_price"
                        android:inputType="number|numberDecimal"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/textView5" />

                    <EditText
                        android:id="@+id/boxSqrFtBox"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:ems="5"
                        android:gravity="center"
                        android:hint="@string/box_sqr_ft"
                        android:inputType="number|numberDecimal"
                        app:layout_constraintStart_toEndOf="@+id/boxPriceBox"
                        app:layout_constraintTop_toTopOf="@+id/boxPriceBox" />

                    <Button
                        android:id="@+id/sqrFtPriceButton"
                        android:layout_width="60dp"
                        android:layout_height="wrap_content"
                        android:layout_margin="8dp"
                        android:text="@string/equals_symbol"
                        android:textColor="@color/black"
                        android:backgroundTint="@color/action_btn_color"
                        app:layout_constraintBottom_toBottomOf="@+id/boxPriceBox"
                        app:layout_constraintStart_toEndOf="@+id/boxSqrFtBox"
                        app:layout_constraintTop_toTopOf="@+id/boxPriceBox" />

                    <TextView
                        android:id="@+id/sqrFtPriceBox"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="12dp"
                        android:gravity="start"
                        android:text="@string/zero"
                        android:textSize="18sp"
                        app:layout_constraintBottom_toBottomOf="@+id/boxPriceBox"
                        app:layout_constraintEnd_toEndOf="@+id/textView5"
                        app:layout_constraintStart_toEndOf="@+id/sqrFtPriceButton"
                        app:layout_constraintTop_toTopOf="@+id/boxPriceBox" />
                </androidx.constraintlayout.widget.ConstraintLayout>


                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15dp"
                    android:background="@color/fading_background"
                    android:orientation="vertical"
                    android:padding="5dp">


                    <TextView
                        android:id="@+id/textView2868"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/n_vertical_louvers"
                        android:textSize="24sp"
                        android:textStyle="bold"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />


                    <EditText
                        android:id="@+id/verticalBlindWidthBox"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:hint="@string/blind_width"
                        android:inputType="number|numberDecimal"
                        app:layout_constraintBottom_toBottomOf="@+id/numOfLouversEqualsButton"
                        app:layout_constraintEnd_toStartOf="@+id/numOfLouversEqualsButton"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="@+id/numOfLouversEqualsButton" />

                    <Button
                        android:id="@+id/numOfLouversEqualsButton"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/equals_symbol"
                        android:textColor="@color/black"
                        android:backgroundTint="@color/action_btn_color"
                        app:layout_constraintEnd_toEndOf="@+id/textView2868"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/textView2868" />

                    <TextView
                        android:id="@+id/numberOfLouversBox"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="@string/zero"
                        android:textSize="18sp"
                        app:layout_constraintBottom_toBottomOf="@+id/numOfLouversEqualsButton"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/numOfLouversEqualsButton"
                        app:layout_constraintTop_toTopOf="@+id/numOfLouversEqualsButton" />

                </androidx.constraintlayout.widget.ConstraintLayout>


                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15dp"
                    android:background="@color/fading_background"
                    android:orientation="vertical"
                    android:padding="5dp">

                    <TextView
                        android:id="@+id/linealBacksplashViewBox"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/lineal_backsplash"
                        android:textSize="24sp"
                        android:textStyle="bold"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <EditText
                        android:id="@+id/bakShWidthBox"
                        android:layout_width="113dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:ems="10"
                        android:gravity="center"
                        android:hint="@string/width_12_in"
                        android:inputType="number|numberDecimal"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/linealBacksplashViewBox" />

                    <EditText
                        android:id="@+id/linealSpaceBox"
                        android:layout_width="121dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:ems="10"
                        android:gravity="center"
                        android:hint="@string/lineal_space"
                        android:inputType="number|numberDecimal"
                        app:layout_constraintStart_toEndOf="@+id/bakShWidthBox"
                        app:layout_constraintTop_toBottomOf="@+id/linealBacksplashViewBox" />

                    <ToggleButton
                        android:id="@+id/bakShToggleButton"
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:lineSpacingExtra="10sp"
                        android:textAllCaps="false"
                        android:textOff="@string/ft."
                        android:textOn="@string/in."
                        android:textSize="14sp"
                        app:layout_constraintBottom_toBottomOf="@+id/linealSpaceBox"
                        app:layout_constraintStart_toEndOf="@+id/linealSpaceBox"
                        app:layout_constraintTop_toTopOf="@+id/linealSpaceBox" />

                    <EditText
                        android:id="@+id/cutOutsBox"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:ems="10"
                        android:gravity="center"
                        android:hint="@string/cut_outs"
                        android:inputType="number|numberDecimal"
                        app:layout_constraintEnd_toStartOf="@+id/bakShEqualsButton"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/bakShWidthBox" />

                    <Button
                        android:id="@+id/bakShEqualsButton"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/equals_symbol"
                        android:textColor="@color/black"
                        android:backgroundTint="@color/action_btn_color"
                        app:layout_constraintBottom_toBottomOf="@+id/cutOutsBox"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/linealSpaceBox" />

                    <TextView
                        android:id="@+id/bakShResultsBox"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="@string/zero"
                        android:textSize="18sp"
                        app:layout_constraintBottom_toBottomOf="@+id/cutOutsBox"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/bakShEqualsButton"
                        app:layout_constraintTop_toTopOf="@+id/cutOutsBox" />

                </androidx.constraintlayout.widget.ConstraintLayout>


                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15dp"
                    android:background="@color/fading_background"
                    android:orientation="vertical"
                    android:padding="5dp">

                    <TextView
                        android:id="@+id/textView20"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/locate_magnet"
                        android:textSize="24sp"
                        android:textStyle="bold"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <EditText
                        android:id="@+id/newMagnetLocationBox"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:hint="@string/new_location"
                        android:inputType="date"
                        app:layout_constraintBottom_toBottomOf="@+id/magnetLocationSaveButton"
                        app:layout_constraintEnd_toStartOf="@+id/magnetLocationSaveButton"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="@+id/magnetLocationSaveButton" />

                    <Button
                        android:id="@+id/magnetLocationSaveButton"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.1"
                        android:text="@string/save"
                        android:textColor="@color/black"
                        android:backgroundTint="@color/action_btn_color"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/textView20" />

                    <TextView
                        android:id="@+id/lastMagnetLocationBox"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="24dp"
                        android:gravity="start"
                        android:text="@string/zero"
                        android:textSize="18sp"
                        app:layout_constraintBottom_toBottomOf="@+id/magnetLocationSaveButton"
